<div class="container-fluid">
  <div class="row">
    <div class="col">
      <div class="container-fluid ml-0 pb-2">
        <div class="row">
          <div class="col nav"><a href="{U_INDEX}">{L_INDEX}</a></div>
        </div>
      </div>
        <div class="p-2 text-center bg-light rounded-2 bdr-lt boxwrapper mx-0">
		<div class="row d-flex bg-body-secondary rounded-2 bdr-cat align-items-center py-1 mx-0"> 
          <div class="col text-center pt-2">{L_VIEWING_PROFILE}</div>
        </div>
        <div class="row mx-0">
          <div class="col-4 lh-lg hr3">
		    <div class="text-center bg-light rounded-2 bdr-lt boxwrapper my-2">
              <div class="col text-center navbr"><strong>&nbsp;{L_AVATAR}</strong></div>
            </div>
			<div class="row d-flex bg-body-secondary rounded-2 bdr-cat align-items-center py-1 mx-0 my-1"> 
              <div class="row text-center">
			    <div class="col">{AVATAR_IMG}</div>
			  </div>
              <div class="row text-center"> 
                <div class="col postdetails">{POSTER_RANK}</div>
              </div>
            </div>
			<div class="text-center bg-light rounded-2 bdr-lt boxwrapper my-2">
              <div class="col text-center navbr"><strong>&nbsp;{L_CONTACT} {USERNAME}</strong></div>
            </div>
			<div class="bg-body-secondary rounded-2 bdr-cat align-items-center py-1 mx-0 my-1"> 
              <div class="row"> 
                <div class="col gen text-end">{L_EMAIL_ADDRESS}:</div>
                <div class="col gen text-start"><strong>{EMAIL_IMG}</strong></div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_PM}:</div>
                <div class="col gen text-start"><strong>{PM_IMG}</strong></div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_FACEBOOK}:</div>
                <div class="col gen text-start">{FB_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_INSTAGRAM}:</div>
                <div class="col gen text-start">{IG_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_PINTEREST}:</div>
                <div class="col gen text-start">{PT_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_TWITTER}:</div>
                <div class="col gen text-start">{TWR_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_SKYPE}:</div>
                <div class="col gen text-start">{SKP_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_TELEGRAM}:</div>
                <div class="col gen text-start">{TG_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_LINKEDIN}:</div>
                <div class="col gen text-start">{LI_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_TIKTOK}:</div>
                <div class="col gen text-start">{TT_IMG}</div>
              </div>
              <div class="row"> 
                <div class="col gen text-end">{L_DISCORD}:</div>
                <div class="col gen text-start">{DC_IMG}</div>
              </div>
            </div>					
          </div>
                
          <div class="col lh-lg">
		    <div class="row text-center bg-light rounded-2 bdr-lt boxwrapper mx-0 my-2">
              <div class="col text-center navbr"><strong>&nbsp;{L_ABOUT_USER}</strong></div>
            </div>
			  <div class="bg-body-secondary rounded-2 bdr-cat align-items-center py-1 mx-0 my-1"> 
                <div class="row"> 
                  <div class="col-2 text-end gen">{L_JOINED}:&nbsp;</div>
                  <div class="col text-start gen"><strong>{JOINED}</strong></div>
                </div>
                <div class="row"> 
                  <div class="col-2 text-end gen">{L_TOTAL_POSTS}:&nbsp;</div>
                  <div class="col text-start gen"><strong>{POSTS}</strong>&nbsp;<span class="genmed text-nowrap">[{POST_PERCENT_STATS} / {POST_DAY_STATS}] <br /><span class="genmed"><a href="{U_SEARCH_USER}" class="genmed">{L_SEARCH_USER_POSTS}</a></div>
                </div>
                <div class="row"> 
                  <div class="col-2 text-end gen">{L_LOCATION}:&nbsp;</div>
                  <div class="col text-start gen"><strong>{LOCATION}</strong></div>
                </div>
                <div class="row"> 
                  <div class="col-2 text-end gen">{L_WEBSITE}:&nbsp;</div>
                  <div class="col text-start gen"><strong>{WWW}</strong></div>
                </div>
                <div class="row"> 
                  <div class="col-2 j text-end gen">{L_OCCUPATION}:&nbsp;</div>
                  <div class="col text-start gen"><strong>{OCCUPATION}</strong></div>
                </div>
                <div class="row"> 
                  <div class="col-2  text-end gen">{L_INTERESTS}:</div>
                  <div class="col text-start gen"> <strong>{INTERESTS}</strong></div>
                </div>
			  </div>
            </div>
          </div>
        </div>
        <div class="container-fluid my-2">
          <div class="row">
            <div class="col gensmall clearfix pr-0" align="right">{S_TIMEZONE}</div>
          </div>
        </div>
        <div class="container-fluid px-0">
          <div class="row">
            <div class="col gen pr-0" align="right">{JUMPBOX}</div>
          </div>
        </div>
    </div>
  </div>
</div>